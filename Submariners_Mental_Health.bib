@article{cymermanSubjectiveSymptomsPostural2002,
  title = {Subjective Symptoms and Postural Control during a Disabled Submarine Simulation.},
  author = {Cymerman, A. and Young, A.J. and Francis, T.J. and Wray, D.D. and Ditzler, D.T. and Stulz, D. and Bovill, M. and Muza, S.R.},
