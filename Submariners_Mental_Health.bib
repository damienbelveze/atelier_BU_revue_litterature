@article{nieExperimentalStudyPhysiological2016b,
  title = {Experimental Study on Physiological Changes of People Trapped in Coal Mine Accidents},
  author = {family=Nie, given=BS, given-i=BS and Huang, X and Sun, X and family=Li, given=AJ, given-i=AJ},
